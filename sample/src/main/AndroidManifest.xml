<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.atermenji.android.iconicdroid.sample"
	android:versionCode="6"
	android:versionName="0.4.0" >

	<uses-sdk
		android:minSdkVersion="8"
		android:targetSdkVersion="17" />

	<application
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme" >

		<activity
			android:name="com.atermenji.android.iconicdroid.sample.MainActivity"
			android:label="@string/app_name" >

			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity
			android:name="com.atermenji.android.iconicdroid.sample.TypefaceSampleActivity"
			android:label="@string/app_name" />

		<activity
			android:name="com.atermenji.android.iconicdroid.sample.SimpleSampleActivity"
			android:label="@string/title_simple_sample"
			android:screenOrientation="portrait" />

	</application>

</manifest>